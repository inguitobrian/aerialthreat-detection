{
  "name": "aerialthreat-detection",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "electron-dev": "concurrently \"npm run dev -- --port 5173\" \"wait-on http://localhost:5173 && electron electron/main.js\"",
    "electron-pack": "npm run build && electron-builder",
    "electron": "electron electron/main.js"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/eslint-plugin": "^1.5.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^7.6.0",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "3.6.2",
    "vite": "^7.2.4",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.14",
    "wait-on": "^7.0.1"
  }
}
